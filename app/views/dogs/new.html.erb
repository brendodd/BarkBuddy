<div class="d-flex justify-content-center my-3">
  <%= simple_form_for(@dog) do |d| %>
    <%= d.input :name %>
    <%= d.input :birthday %>
    <%= d.input :size %>
    <%= d.input :breed %>
    <%= d.input :address %>
    <%= d.input :photo, as: :file, label: "Picture of your Dog" %>
    <%= d.submit "Add Your Dog!" %>
  <% end %>
</div>
